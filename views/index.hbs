<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>
<button type="button" class="btn btn-primary btn-lg">New User</button>
<table class="table table-striped">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Username</th>
      <th>Email</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    {{#each users as |user|}}
    <tr>
      <td scope="row">{{user.dataValues.fname}}</td>
      <td>{{user.dataValues.lname}}</td>
      <td>{{user.dataValues.username}}</td>
      <td>{{user.dataValues.email}}</td>
      <td>edit</td>
      <td>delete</td>
    </tr>
    {{/each}}
  </tbody>
</table>
